package kerosene;

public class Util {
	private int rate;
	
	Util (int rate) {
		this.rate = rate;
	}
	
	public int getRate () {
		return this.rate;
	}
	
	public void setRate(int rate) {
		this.rate = rate;
	}
	
	public static int charge (int litre, int rate) {
		int amount = litre * rate;
		return litre = amount;
	}
	
}
